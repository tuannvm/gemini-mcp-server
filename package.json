{
  "name": "@tuannvm/gemini-mcp-server",
  "version": "1.1.0",
  "description": "MCP server for Gemini CLI integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gemini-mcp-server": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts' --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "gemini",
    "cli",
    "ai",
    "llm"
  ],
  "author": "tuannvm",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuannvm/gemini-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/tuannvm/gemini-mcp-server/issues"
  },
  "homepage": "https://github.com/tuannvm/gemini-mcp-server#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.0",
    "chalk": "^5.6.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
